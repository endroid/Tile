{
    "name": "endroid/tile",
    "description": "Endroid Tile",
    "keywords": ["endroid", "delft", "blue", "tile", "php"],
    "homepage": "https://github.com/endroid/tile",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van den Enden",
            "email": "info@endroid.nl"
        }
    ],
    "require": {
        "php": ">=7.2",
        "endroid/installer": "^1.0.3",
        "imagine/imagine": "^0.6",
        "symfony/routing": "^3.4||^4.0||^5.0",
        "twig/twig": "^1.34||^2.0||^3.0"
    },
    "require-dev": {
        "endroid/test": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Endroid\\Tile\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
